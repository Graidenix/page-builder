{
  "display": "Node LTS",
  "_version": "19.8.1",
  "compilerOptions": {
    "lib": ["ES2021"],
    "module": "Node12",
    "target": "ES2022",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node12"
  },
  "include": [
    "src"
  ]
}
